<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
  <!-- primary -->
  <Color x:Key="PrimaryHueMidColor" A="255" R="93" G="2" B="153" />
  <Color x:Key="PrimaryHueMidForegroundColor" A="255" R="255" G="255" B="255" />
  <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#C35EFF" />
  <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#FFFFFF" />
  <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#5D0299" />
  <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFF" />
  <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#450185" />
  <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF" />
  <!-- accent -->
  <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#E2FF5E" />
  <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#FFFFFF" />
  <!-- stage -->
  <SolidColorBrush x:Key="Added" Color="#E2FF5E" />
  <SolidColorBrush x:Key="Removed" Color="#FFC85A5A" />
  <SolidColorBrush x:Key="Changed" Color="#FFF8F85A" />
  <SolidColorBrush x:Key="Untracked" Color="#FF5AC85A" />
</ResourceDictionary>
